#!/usr/bin/env perl6

use lib <uni>;
use Galaxy::Physics;

my $physics   = Galaxy::Physics.new;

my $alien     = $physics.alien;
my $galaxy    = $physics.galaxy;
my $gravity   = $physics.gravity;
my $blackhole = $physics.blackhole;
my $spacetime = $physics.spacetime;
my $nebulas   = $physics.nebulas;

say $galaxy.stars;
